{
  "name": "tokyo",
  "version": "1.0.0",
  "main": "dist/server.js",
  "engines": {
    "node": "22.x"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "start:render": "node dist/server.js",
    "dev": "nodemon server.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "db:seed": "ts-node prisma/seed.ts",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "deploy:render": "git push origin main",
    "postinstall": "prisma generate",
    "render:build": "npm install && npm run db:generate && npm run build",
    "client:install": "cd client && npm install",
    "client:start": "cd client && npm start",
    "client:build": "cd client && npm run build",
    "dev:all": "concurrently \"npm run dev\" \"npm run client:start\""
  },
  "keywords": [
    "coffee-chat",
    "overseas-employment",
    "community"
  ],
  "author": "",
  "license": "ISC",
  "description": "해외 취업 커피챗 커뮤니티",
  "dependencies": {
    "@prisma/client": "^6.14.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "@types/pg": "^8.15.5",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "pg": "^8.16.3",
    "prisma": "^6.14.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "nodemon": "^3.0.2"
  }
}
