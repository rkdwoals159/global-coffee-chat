{
  "functions": {
    "api/coffee-chats/index.js": {
      "runtime": "nodejs18.x"
    },
    "api/coffee-chats/[id].js": {
      "runtime": "nodejs18.x"
    },
    "api/coffee-chats/country/[country].js": {
      "runtime": "nodejs18.x"
    },
    "api/coffee-chats/job/[job].js": {
      "runtime": "nodejs18.x"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "buildCommand": "cd client && npm run build",
  "outputDirectory": "client/build"
}
